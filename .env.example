# ============================================
# A2A Multi-Agent System Configuration
# ============================================

# --------------------------------------------
# Google AI Configuration
# --------------------------------------------
# Choose one of the following options:

# Option 1: Gemini Developer API (recommended for development)
GOOGLE_API_KEY="your-api-key-here"

# Option 2: Vertex AI (recommended for production)
GOOGLE_GENAI_USE_VERTEXAI=TRUE
GOOGLE_CLOUD_PROJECT="your-project-id"
GOOGLE_CLOUD_LOCATION="global"
GOOGLE_APPLICATION_CREDENTIALS="/path/to/your/credentials.json"

# Model Configuration
GOOGLE_GENAI_MODEL="gemini-2.5-flash"

# --------------------------------------------
# LiteLLM Settings (Alternative LLM)
# --------------------------------------------
LITELLM_MODEL="dashscope/qwen-plus"
DASHSCOPE_API_BASE="https://dashscope.aliyuncs.com/compatible-mode/v1"

# --------------------------------------------
# Tripadvisor Settings
# --------------------------------------------
TRIPADVISOR_API_KEY="your-tripadvisor-api-key"

# --------------------------------------------
# Logging Configuration
# --------------------------------------------
LOG_LEVEL=info
